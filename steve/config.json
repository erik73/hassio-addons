{
  "name": "SteVe",
  "version": "0.2.0",
  "slug": "steve",
  "description": "OCPP server for EV charging stations",
  "url": "https://github.com/erik73/hassio-addons",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "stage": "experimental",
  "startup": "application",
  "init": false,
  "ingress": false,
  "panel_icon": "mdi:CarElectric",
  "boot": "auto",
  "map": ["ssl"],
  "services": ["mysql:need"],
  "host_network": true,
  "hassio_api": true,
  "hassio_role": "default",
  "options": {
    "timezone": "CET",
    "httpport": "8180",
    "admin_user": "admin",
    "admin_password": null
  },
  "schema": {
    "timezone": "str",
    "httpport": "str",
    "admin_user": "str",
    "admin_password": "str",
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?"
  },
  "image": "ehilton73/steve-{arch}"
}
